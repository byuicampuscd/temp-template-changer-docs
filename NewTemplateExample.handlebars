<!DOCTYPE html>
<html lang="en-us">
<head>
    <meta charset="utf-8">
    <title>{{title}}</title>
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <link href="Web%20Files/course.css" rel="stylesheet">
</head>
<body>
        {{#if isSplash}}
        <div id="main" class="splash">
        <div id="header">
            <img alt="Course Banner" src="Web%20Files/largeBanner.png" />
        </div>
        {{else}}
        <div id="main" class="splash">
        <div id="header">
            <img alt="Course Banner" src="Web%20Files/smallBanner.png" />
        </div>
        {{/if}}
        <div id="welcome">
            <img alt="Welcome Image" src="{{welcomeSrc}}" />
        </div>
        <div id="article">
            {{{middle}}}
            <div>
                {{#each paragraphs}}
                {{this}}
                {{/each}}
            </div>
        </div>
    </div>
    <script src="Web%20Files/course.js"></script>
</body>
</html>
